    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^5.3.0",
    "source-map-support": "^0.5.21",
    "symbol-es6": "^0.1.2",
    "terser": "^5.16.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "whatwg-fetch": "^3.0.0"
